<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite" verbose="1" >
    <parameter name="url" value="https://www.lavineteca.com/"/>
    <test name="All Test chrome of Flujo de verificacion del menu de opciones de la parte del usuario">
        <parameter name="browser" value="chrome"/>
        <packages>
            <package name="orderMenu"/>
        </packages>
    </test>
    <test name="All Test firefox of Flujo de verificacion del menu de opciones de la parte del usuario">
        <parameter name="browser" value="firefox"/>
        <packages>
            <package name="orderMenu"/>
        </packages>
    </test>
</suite>